<%- include('./layouts/header') %>

<div class="content-wrapper">
  <div class="container-fluid">
    <div class="card mb-3">
      <div class="card-header">
        <i class="fa fa-table"></i>
        <%= tbname %>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-striped" elc-js="SelectResultTbl" width="100%"  cellspacing="0">
            <thead>
              <tr>
                <th>Column Name</th>
                <th>Data Type</th>
                <th>Description</th>
              </tr>
            </thead>
            <tfoot>
                <th>Column Name</th>
                <th>Data Type</th>
                <th>Description</th>
            </tfoot>
            <tbody>
              <% rows.forEach(function(row) { %>
                <tr>
                  <td><%= row['name'] %></td>
                  <td><%= row['type'] %></td>
                  <td><%= row['description'] %></td>
                </tr>
              <% }); %>
            </tbody>
          </table>
        </div>
      </div>
      <div class="card-footer small text-muted">
        Updated yesterday at 11:59 PM
      </div>
    </div>

  </div>
  <!-- /.container-fluid -->
</div>
<!-- /.content-wrapper -->

<%- include('./layouts/footer') %>
